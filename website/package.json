{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "generate:clean": "del docs/api",
    "generate:api:node": "yarn --cwd ../packages/repack typedoc --options typedoc.node.json && mv ./docs/api/node/README.md ./docs/api/node/index.md",
    "generate:api:react-native": "yarn --cwd ../packages/repack typedoc --options typedoc.react-native.json && mv ./docs/api/react-native/README.md ./docs/api/react-native/index.md",
    "generate:diff": "mkdir -p tmp && git diff origin/nativepack:templates/webpack.config.js origin/main:templates/webpack.config.js > tmp/repack_1-2.diff",
    "generate": "yarn generate:clean && yarn generate:api:node && yarn generate:api:react-native && yarn generate:diff",
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.5",
    "@docusaurus/preset-classic": "2.0.0-beta.5",
    "@mdx-js/react": "^1.6.21",
    "@svgr/webpack": "^5.5.0",
    "clsx": "^1.1.1",
    "del-cli": "^4.0.1",
    "file-loader": "^6.2.0",
    "prism-react-renderer": "^1.2.1",
    "raw-loader": "^4.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "url-loader": "^4.1.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@callstack/eslint-config": "^11.0.0",
    "eslint": "^7.32.0",
    "netlify-cli": "^6.7.7"
  }
}
